{
  "$schema": "https://raw.githubusercontent.com/lukaw3d/parcel-bundler-json-schemas/main/package_schema.json",
  "name": "explorer-frontend",
  "version": "0.0.0-development",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/oasisprotocol/explorer.git"
  },
  "scripts": {
    "build": "node ./internals/scripts/build.js",
    "clean": "rm -rf build/ .parcel-cache",
    "start": "parcel --dist-dir build-dev --no-cache",
    "checkTs": "tsc --noEmit",
    "lint": "prettier src internals --check",
    "lint:fix": "prettier src internals --write",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "targets": {
    "web": {
      "source": "public/index.html",
      "distDir": "./build",
      "context": "browser",
      "scopeHoist": false
    }
  },
  "browserslist": {
    "production": [
      "defaults",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/@emotion/styled": "^11.10.5",
    "react-refresh": "0.9.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/figtree": "^4.5.3",
    "@fontsource/roboto-mono": "^4.5.10",
    "@metamask/jazzicon": "^2.0.0",
    "@mui/base": "^5.0.0-alpha.111",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@oasisprotocol/client": "0.1.1-alpha.2",
    "@oasisprotocol/client-rt": "0.2.1-alpha.2",
    "@tanstack/react-query": "^4.19.1",
    "@tanstack/react-query-devtools": "^4.20.4",
    "axios": "^1.2.1",
    "bignumber.js": "^9.1.1",
    "date-fns": "^2.29.3",
    "i18next": "^22.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1",
    "react-quick-pinch-zoom": "^4.6.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.5",
    "recharts": "^2.2.0",
    "swiper": "^9.0.4",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@emotion/jest": "^11.10.5",
    "@parcel/packager-raw-url": "^2.8.1",
    "@parcel/transformer-webmanifest": "^2.8.1",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-viewport": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "buffer": "^5.5.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "parcel": "^2.8.1",
    "prettier": "^2.8.1",
    "process": "^0.11.10",
    "react-app-polyfill": "^3.0.0",
    "storybook-addon-react-router-v6": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
