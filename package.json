{
  "$schema": "https://raw.githubusercontent.com/lukaw3d/parcel-bundler-json-schemas/main/package_schema.json",
  "name": "explorer-frontend",
  "version": "0.0.0-development",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/oasisprotocol/explorer.git"
  },
  "scripts": {
    "build": "node ./internals/scripts/build.js",
    "build-preview": "REACT_APP_BUILD_PREVIEW=preview  yarn run build",
    "build-internal": "REACT_APP_BUILD_PREVIEW=internal  yarn run build",
    "clean": "rm -rf build/ .parcel-cache",
    "start": "parcel --dist-dir build-dev --no-cache",
    "checkTs": "tsc --noEmit",
    "lint": "eslint --ext js,ts,tsx ./",
    "lint:fix": "yarn run lint --fix",
    "prettify": "prettier src internals --write",
    "test": "jest",
    "start-storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "targets": {
    "web": {
      "source": "public/index.html",
      "distDir": "./build",
      "context": "browser",
      "scopeHoist": false
    }
  },
  "browserslist": {
    "production": [
      "defaults",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/@emotion/styled": "^11.10.6",
    "react-refresh": "0.14.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "11.10.6",
    "@ethereumjs/util": "^8.0.5",
    "@fontsource/figtree": "^4.5.3",
    "@fontsource/roboto-mono": "^4.5.10",
    "@metamask/jazzicon": "^2.0.0",
    "@mui/base": "5.0.0-alpha.119",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "5.11.11",
    "@oasisprotocol/client": "0.1.1-alpha.2",
    "@oasisprotocol/client-rt": "0.2.1-alpha.2",
    "@tanstack/react-query": "4.29.3",
    "@tanstack/react-query-devtools": "^4.26.1",
    "axios": "^1.3.4",
    "bignumber.js": "^9.1.1",
    "date-fns": "^2.29.3",
    "i18next": "^22.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-quick-pinch-zoom": "^4.6.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "recharts": "^2.5.0",
    "swiper": "^9.1.1",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@emotion/jest": "^11.10.5",
    "@parcel/packager-raw-url": "^2.8.3",
    "@parcel/transformer-webmanifest": "^2.8.3",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-viewport": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-webpack5": "^7.0.2",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "buffer": "^5.7.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.8.3",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "react-app-polyfill": "^3.0.0",
    "storybook": "^7.0.2",
    "storybook-addon-react-router-v6": "^0.3.5",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.0"
  }
}
